datatypes xsd = "http://www.w3.org/2001/XMLSchema-datatypes"

start = element Adapter { dns }
	
# CGA-TSIG Param
cga-tsig = element CGA-TSIG {
        element Algorithm { xsd:string },
        element AlgorithmKey {  xsd:positiveInteger },
        element Modifier {  xsd:base64Binary },
        element CollisionCount {  xsd:positiveInteger },
        element KeyPath { xsd:string },
        element Keysize {  xsd:positiveInteger },
	element version {  xsd:positiveInteger },
        element RemoteAddress { xsd:string },
}


