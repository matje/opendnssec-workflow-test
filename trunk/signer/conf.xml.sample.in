<?xml version="1.0" encoding="UTF-8"?>

<!-- $Id$ -->

<Configuration>

	<RepositoryList>
		<Repository>
			<Name>softHSM</Name>
			<Module>@prefix@/lib/libsofthsm.so</Module>
			<PIN>test:5678</PIN>
		</Repository>
	</RepositoryList>

	<Enforcer>
		<Interval>PT3600S</Interval>
		<KeygenInterval>P3M</KeygenInterval>
		<BackupDelay>P3D</BackupDelay>
	</Enforcer>

	<Signer>
		<WorkingDirectory>@localstatedir@/opendnssec/tmp</WorkingDirectory>
		<ZoneListFile>@sysconfdir@/opendnssec/zonelist.xml</ZoneListFile>
		<!-- toolsdir will be removed, but is used for testing atm -->
		<ToolsDirectory>@prefix@/libexec/opendnssec</ToolsDirectory>
	</Signer>

</Configuration>
